    <!-- Form Reg -->
    <section class="reg">
        <div class="container" id="ProjectsForm">
            <div class="title"><i class="fa fa-briefcase"></i>Projects</div>
            <div class="content">
                <form id="Projects" method="post" enctype="multipart/form-data">
                    <div class="user-details" id="Projectssuser">
                        <div class="input-box full">
                            <span class="details">Client Name</span>
                            <input type="text" autocomplete='off' name="ClientName" class="ClientName">
                        </div>
                        <div class="input-box full">
                            <span class="details">Category</span>
                            <input type="text" autocomplete='off' name="Category" class="Category">
                        </div>
                        <div class="input-box full">
                            <span class="details">Description</span>
                            <textarea autocomplete='off' name="Description" class="Description"></textarea>
                        </div>
                        <div class="input-box margin0 full">
                            <span class="details">End Date</span>
                            <select name="EndDate" id="date" class="form-control EndDate">
                                <option value="2021" selected>2021</option>
                                @for (int i = 2020; i >= 1990; i--)
                                {
                                    <option value="@i">@i</option>
                                }
                            </select>
                        </div>
                        <div class="input-box ">
                            <span class="details">Project Name</span>
                            <input type="text" autocomplete='off' name="ProjectName" class="ProjectName">
                        </div>
                        <div class="input-box">
                            <span class="details">Tools</span>
                            <input type="text" required autocomplete='off' name="Tools" class="Tools">
                        </div>
                        <div class="input-box full">
                            <span class="details">Domain Name</span>
                            <input type="text" required autocomplete='off' name="DomainName" class="DomainName">
                        </div>

                        <div class="input-box file">
                            <label>Interface Image</label>
                            <input type="file" autocomplete='off' name="InterfaceImage" class="InterfaceImage">
                        </div>
                        <div class="input-box file">
                            <label>Sub Image</label>
                            <input type="file" multiple autocomplete='off' name="SubImage" class="SubImage">
                        </div>
                    </div>
                    <div class="buttonADD" id="button4">
                        <div class="next">
                            <input type="submit" value="Add More Projects" id="Finsh">
                        </div>
                    </div>
                </form>
            </div>
        </div>


        <div class="button">
            <div class="back">
                <span class="material-icons">
                    arrow_back
                </span>
                <input type="button" value="Back" onclick="location.href='@Url.Action("PersonalWorkExperience", "PersonalWorkExperience")'">
            </div>

            <div class="next">
                <input type="button" value="Finsh" id="save" onclick="location.href='@Url.Action("PersonalCV", "PersonalCV")'">
                <span class="material-icons" id="next">
                    arrow_forward
                </span>
            </div>
        </div>
    </section>
